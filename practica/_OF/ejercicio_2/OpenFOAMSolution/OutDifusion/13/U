FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "13";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    xmin
    {
        type            zeroGradient;
    }
    xmax
    {
        type            zeroGradient;
    }
    ymin
    {
        type            zeroGradient;
    }
    ymax
    {
        type            zeroGradient;
    }
    zmin
    {
        type            empty;
    }
    zmax
    {
        type            empty;
    }
}


